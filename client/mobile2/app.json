{
  "name": "Free And Fair Elections",
  "displayName": "Free And Fair Elections",
  "expo": {
    "name": "Free And Fair Elections",
    "icon": "./assets/FreeAndFair2020-icon4.png",
    "description": "Crowdsourced Election Integrity. Keeping elections safe, free, fair. Keeping voters and media informed about what's happening when there's trouble.",
    
    "version": "1.1.0",
    
    "slug": "free-and-fair-elections",
    "orientation": "portrait",

    "assetBundlePatterns": [
      "**/*"
    ],

    "splash": {
      "image": "./assets/FreeAndFair2020-splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#206182"
    },

    "ios": {
      "bundleIdentifier": "com.freeandfairelections.freeandfair",
      "buildNumber": "1.1.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to record posted poll tapes, or capture other election integrity related incidents."
      }
    },
    "android": {
      "package": "com.freeandfairelections.freeandfair",
      "versionCode": 4,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COURSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
